html
  head
    title Rdio Music Playback
    script(type='text/javascript' src='/assets/js/jquery-1.5.1.min.js')
    script(type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/swfobject/2.2/swfobject.js')
    script(type='text/javascript' src='/assets/js/token.js')
    script(type='text/javascript' src='/assets/js/hello.js')
    //script(src="/assets/client.js")
    link(rel='stylesheet', type='text/css', href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css')
    link(href='//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css', rel='stylesheet')
    link(rel='stylesheet', type='text/css', href='stylesheets/global.css')
  body
    section.lead
      a(href="/maker") Home
    .container
      #apiswf
      .well(style='overflow: hidden;')
        .row(style='margin-bottom: 10px;')
          .col-md-2 Enter Album Key
          .col-md-10
            input#play_key.form-control(value='p12763082')
        .row
          .col-md-6
            .playControls.btn-group
              button#play.btn.btn-default
                i.fa.fa-play-circle
                |  Play
              button#shuffleOn.btn.btn-default
                i.fa.fa-shuffleOn
                |  Shuffle On
              button#shuffleOff.btn.btn-default
                i.fa.fa-shuffleOff
                |  ShuffleOff
              button#stop.btn.btn-default
                i.fa.fa-stop
                |  Stop
              button#pause.btn.btn-default
                i.fa.fa-pause
                |  Pause
              button#previous.btn.btn-default
                i.fa.fa-step-backward
                |  Previous
              button#next.btn.btn-default
                i.fa.fa-step-forward
                |  Next
          .col-md-6
            #freq.frequency
              div
              div
              div
              div
              div
              div
              div
              div
              div
              div
      .row.well
        .artistAlbumInfo.well.col-md-6
          h3
            span#artist
            |  - 
            span#track
        .col-md-6
          h3#album
          h4
            | Playstate 
            p#playState
          h4
            | Position 
            p#position
